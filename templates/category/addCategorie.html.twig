{% extends "main/base.html.twig " %}
{% set title="Bucket-List - Ajoute Categorie" %}

{% block title %}
	{{title}}
{% endblock %}

{% block main %}
	<link rel="stylesheet" href="{{ asset('css/formulaire.css') }}">
	<div class="title">
		{{title}}</div>
	<div class="login-box">
		<h2>Categorie</h2>

		{{ form_start(categorieForm) }}
		<div class="user-box">
			{{ form_row(categorieForm.name) }}
		</div>

		<button class="validation" type="submit">Valider
		</button>

		{{ form_end(categorieForm) }}

	</div>
{% endblock %}
